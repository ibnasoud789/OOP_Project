            <Font name="System Bold" size="16.0" />
         </font>
      </Button>
      <Button fx:id="goingBackOption" layoutX="706.0" layoutY="460.0" mnemonicParsing="false" onAction="#gotothePreviousPage" prefHeight="32.0" prefWidth="81.0" text="Go Back">
         <font>
            <Font name="System Bold" size="14.0" />
         </font>
      </Button>
      <MediaView fx:id="mediaview" fitHeight="200.0" fitWidth="200.0" layoutX="311.0" layoutY="209.0" onError="#mediaviewOnAction" />
   </children>
</AnchorPane>
