<AnchorPane id="AnchorPane" prefHeight="539.0" prefWidth="846.0" style="-fx-background-color: Black;" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="erin.NusratContentStreamController_1">
   <children>
      <Label layoutX="299.0" layoutY="41.0" text="Stream Your Content" textFill="WHITE">
         <font>
            <Font name="System Bold" size="25.0" />
         </font>
      </Label>
      <Label layoutX="62.0" layoutY="114.0" text="Choose from files:" textFill="WHITE">
         <font>
            <Font name="System Bold" size="22.0" />
         </font>
      </Label>
      <Button fx:id="chooseFile" layoutX="272.0" layoutY="115.0" mnemonicParsing="false" onAction="#choosefromPC" text="Choose from files." />
      <Label layoutX="62.0" layoutY="155.0" text="Category:" textFill="WHITE">
         <font>
            <Font name="System Bold" size="22.0" />
         </font>
      </Label>
      <ComboBox fx:id="selectCategory" layoutX="272.0" layoutY="156.0" prefWidth="150.0" />
      <Button fx:id="startStreamNow" layoutX="349.0" layoutY="458.0" mnemonicParsing="false" onAction="#streamPage" style="-fx-background-color: Maroon;" text="Start Streaming!" textFill="#fcf0f0">
